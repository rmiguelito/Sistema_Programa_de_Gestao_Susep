{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "File", "Args": { "path": "Logs/log.txt" } }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "identityServer": {
    "jwtBearerOptions": {
      "authority": "http://caminhointernoservicos/",
      "requireHttpsMetadata": false,
      "audience": "SISGP.APIGateway"
    }
  }
}
